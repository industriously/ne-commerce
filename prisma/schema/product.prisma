model Product {
  id          String   @id @default(uuid())
  name        String
  description String
  price       Int
  created_at  DateTime @default(now())
  updated_at  DateTime @default(now())
  is_deleted  Boolean  @default(false)
  store_id    String
  store       Store    @relation(fields: [store_id], references: [id])

  @@map("products")
}
